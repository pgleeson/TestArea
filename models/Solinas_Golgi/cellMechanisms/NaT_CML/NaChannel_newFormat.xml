<?xml version="1.0" encoding="UTF-8"?>
<channelml xmlns="http://morphml.org/channelml/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:meta="http://morphml.org/metadata/schema" xsi:schemaLocation="http://morphml.org/channelml/schema  ../../Schemata/v1.7.3/Level2/ChannelML_v1.7.3.xsd" units="Physiological Units">

    <meta:notes>ChannelML file containing a single Channel description</meta:notes>

    <channel_type name="NaT_CML" density="yes">
    
        <status value="in_progress">
	           
	        </status>
	        
	        <meta:notes>Na channel</meta:notes>
	    
	        <meta:authorList>
	            <meta:modelTranslator>
	                <meta:name>Padraig Gleeson</meta:name>
	                <meta:institution>UCL</meta:institution>    
	                <meta:email>p.gleeson - at - ucl.ac.uk</meta:email>
	            </meta:modelTranslator>
	        </meta:authorList>
	        
	            <!--
	        
	        <meta:publication>
	           <meta:fullTitle>Maex, R and De Schutter, E. 
	           Synchronization of Golgi and Granule Cell Firing in a Detailed Network Model of the 
	           cerebellar Granule Cell Layer. J Neurophysiol, Nov 1998; 80: 2521 - 2537</meta:fullTitle>
	           <meta:pubmedRef>http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&amp;db=PubMed&amp;list_uids=9819260&amp;dopt=Abstract</meta:pubmedRef>
	        </meta:publication>     
	   
	        <meta:neuronDBref>
	            <meta:modelName>K channels</meta:modelName>
	            <meta:uri>http://senselab.med.yale.edu/senselab/NeuronDB/channelGene2.htm#table3</meta:uri>
	        </meta:neuronDBref>
	        
	        -->
	
	
	        
	        <current_voltage_relation cond_law="ohmic" ion="na" default_gmax="48" default_erev="-85">
	            
	            <q10_settings q10_factor="3" experimental_temp="20"/>	            	            
	          
	                <!--
	                Aalpha_m = 0.3 (/ms-mV)
			Kalpha_m = -10 (mV)
			V0alpha_m = -25 (mV)
			
			alp_m = tcorr*Aalpha_m*linoid(v-V0alpha_m,Kalpha_m)
						FUNCTION linoid(x (mV),y (mV)) (mV) {
							if (fabs(x/y) < 1e-6) {
								linoid = y*(1 - x/y/2)
							}else{
								linoid = x/(1 - exp(x/y))
							}
						}
						
			alp_m = Aalpha_m * (-Kalpha_m) * (v-V0alpha_m)/(-Kalpha_m) / (1 - exp(-(v-V0alpha_m)/(-Kalpha_m)))	
			
			alpha =  A * ((v-V1/2)/B)  / (1 - exp(-(v-V1/2)/B))  (exp_linear)


			Abeta_m = 12 (/ms)
			Kbeta_m = -18.182 (mV)
			V0beta_m = -50 (mV)
			
			bet_m = tcorr*Abeta_m*exp((v-V0beta_m)/Kbeta_m) 


			Aalpha_h  = 0.21 (/ms)
			Kalpha_h  = -3.333 (mV)
			V0alpha_h = -50 (mV)
			
			alp_h = tcorr*Aalpha_h*exp((v-V0alpha_h)/Kalpha_h) 


			Abeta_h  = 3 (/ms)
			Kbeta_h  = -5 (mV)
			V0beta_h = -17 (mV)
			
			bet_h = tcorr*Abeta_h/(1+exp((v-V0beta_h)/Kbeta_h))
			
			beta = A * exp((v-V1/2)/B)   (exponential)
			
			
			
	                -->
            
            <gate name="m" instances="3">
                <closed_state id="m0"/>
                <open_state id="m"/>

                <transition name="alpha" from="m0" to="m" expr_form="exp_linear" rate="3" scale="10" midpoint="-25"/>  
                <transition name="beta" from="m" to="m0" expr_form="exponential" rate="12" scale="-18.182 " midpoint="-50"/>  

            </gate>
            
	    <gate name="h" instances="1">
		<closed_state id="h0"/>
		<open_state   id="h"/>

		<transition name="alpha" from="h0" to="h" expr_form="exponential" rate="0.21" scale="-3.333" midpoint="-50"/>  
		<transition name="beta"  from="h" to="h0" expr_form="generic" expr="3/(1+exp((v+17)/-5))"/>    
            </gate>
            
        </current_voltage_relation>
        
	<impl_prefs>
		    <table_settings max_v="30" min_v="-100" table_divisions="13000"/>
	</impl_prefs>

        
    </channel_type>
</channelml>