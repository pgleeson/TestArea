<?xml version="1.0" encoding="UTF-8"?>
<channelml xmlns="http://morphml.org/channelml/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:meta="http://morphml.org/metadata/schema" xsi:schemaLocation="http://morphml.org/channelml/schema  ../../Schemata/v1.7.3/Level2/ChannelML_v1.7.3.xsd" units="Physiological Units">

    <channel_type name="KC_CML" density="yes">
    
    <status value="in_progress">
               
            </status>
            
        <meta:notes>Calcium dependent K channel</meta:notes>
        
            <meta:authorList>

                  <meta:modelTranslator>
                <meta:name>Matteo Farinella</meta:name>
                <meta:institution>UCL</meta:institution>    
                <meta:email>m.farinella - at - ucl.ac.uk</meta:email>
            </meta:modelTranslator>

             <meta:modelAuthor>
                <meta:name>E.DAngelo</meta:name>  
                <meta:comment>Cited implementer according to the original .mod file</meta:comment>
            </meta:modelAuthor>

            <meta:modelAuthor>
                <meta:name>T.Nieus</meta:name>
                <meta:comment>Cited implementer according to the original .mod file</meta:comment>
            </meta:modelAuthor>

          <meta:modelAuthor>
                <meta:name>A. Fontana</meta:name>
                <meta:comment>Cited implementer according to the original .mod file</meta:comment>
            </meta:modelAuthor>
           </meta:authorList>

         <meta:publication>
            <meta:fullTitle>Solinas S, Forti L, Cesana E, Mapelli J, De Schutter E, D'Angelo E. (2007) Computational reconstruction of pacemaking and intrinsic electroresponsiveness in cerebellar Golgi cells.  Front Cell Neurosci. 2007;1:2.</meta:fullTitle>
            <meta:pubmedRef>http://www.ncbi.nlm.nih.gov/pubmed/18946520</meta:pubmedRef>
        </meta:publication>

        <meta:neuronDBref>
            <meta:modelName>Ca channels</meta:modelName>
            <meta:uri>http://senselab.med.yale.edu/NeuronDB/ChannelGene2.htm#table1</meta:uri>
        </meta:neuronDBref>

        <meta:modelDBref>
            <meta:modelName>Cerebellar Golgi cell (Solinas et al. 2007a, 2007b)</meta:modelName>
            <meta:uri>http://senselab.med.yale.edu/modeldb/ShowModel.asp?model=112685</meta:uri>
        </meta:modelDBref>

       <current_voltage_relation cond_law="ohmic" ion="k" default_gmax="9" default_erev = "-85">>
            
            <conc_dependence name="Calcium" ion="ca" variable_name="ca_conc" /> <!--min_conc="7.55e-7" max_conc="0.050"/>  -->
                
            <q10_settings q10_factor="3" experimental_temp="30"/>
                        
            <gate name="c" instances="1">

                <closed_state id="c0"/>
                <open_state id="c"/>
                                
		<!--
			Aalpha_c = 7 (/ms)
			Balpha_c = 1.5e-3 (mM)

			Kalpha_c =  -11.765 (mV)

			Abeta_c = 1 (/ms)
			Bbeta_c = 0.15e-3 (mM)

			Kbeta_c = -11.765 (mV)
			
			alp_c = tcorr*Aalpha_c/(1+(Balpha_c*exp(v/Kalpha_c)/cai))
				7/(1 + (1.5e-3 * exp(v/-11.765)/ca_conc))
				
			bet_c = tcorr*Abeta_c/(1+cai/(Bbeta_c*exp(v/Kbeta_c)))
				1/(1 + cai/(0.15e-3*exp(v/-11.765))

                -->
                     
                <transition name="alpha" from="c0" to="c" expr_form="generic" expr="temp_adj_c*7/(1 + (0.0015 * exp(-v/11.765)/ca_conc))"/>
                <transition name="beta" from="c" to="c0" expr_form="generic" expr="temp_adj_c*1/(1 + ca_conc/(0.00015*exp(-v/11.765)))"/>

                <time_course name="tau" from="c0" to="c" expr_form="generic" expr="temp_adj_c/(alpha+beta)"/> 

                    
            </gate>
        </current_voltage_relation> 
        
     <impl_prefs>
		<table_settings max_v="30" min_v="-100" table_divisions="13000"/>
	</impl_prefs>
        
    </channel_type>
</channelml>