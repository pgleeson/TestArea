<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>neuroConstruct</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>neuroConstruct</name>
            <properties>
                <property name="project.dir">..</property>
                <property name="ant.script">${project.dir}/build.xml</property>
            </properties>
            <folders>
                <source-folder>
                    <label>neuroConstruct</label>
                    <location>${project.dir}</location>
                </source-folder>
                <source-folder>
                    <label>neuroConstruct</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>src</label>
                    <type>java</type>
                    <location>${project.dir}/src</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>nbproject/nbjdk.xml</script>
                    <target>compile</target>
                </action>
                <action name="clean">
                    <script>nbproject/nbjdk.xml</script>
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <script>nbproject/nbjdk.xml</script>
                    <target>javadoc</target>
                </action>
                <action name="run">
                    <script>nbproject/nbjdk.xml</script>
                    <target>runbiglast</target>
                </action>
                <action name="rebuild">
                    <script>nbproject/nbjdk.xml</script>
                    <target>clean</target>
                    <target>compile</target>
                </action>
                <action name="run.single">
                    <script>nbproject/nbjdk.xml</script>
                    <target>run-selected-file-in-src</target>
                    <context>
                        <property>run.class</property>
                        <folder>${project.dir}/src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="debug">
                    <script>nbproject/nbjdk.xml</script>
                    <target>debug-nb</target>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>${project.dir}/classes</location>
                <script>nbproject/nbjdk.xml</script>
                <build-target>compile</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>src</label>
                        <location>${project.dir}/src</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="run"/>
                    <ide-action name="debug"/>
                    <action>
                        <script>${ant.script}</script>
                        <label>run no load</label>
                        <target>run</target>
                    </action>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>${project.dir}/src</package-root>
                <classpath mode="compile">${project.dir}/lib/hdf5/jhdf.jar;${project.dir}/lib/hdf5/jhdf4obj.jar;${project.dir}/lib/hdf5/jhdf5.jar;${project.dir}/lib/hdf5/jhdf5obj.jar;${project.dir}/lib/hdf5/jhdfobj.jar</classpath>
                <built-to>${project.dir}/classes</built-to>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
